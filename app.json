{
  "expo": {
    "name": "MACROMATE",
    "slug": "macromate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logos/Primary_Emerald.png",
    "scheme": "macromate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/images/Logos/Monochrome_Dark.png",
        "light": "./assets/images/Logos/Primary_Emerald.png",
        "tinted": "./assets/images/Logos/App_Icon_Variant.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#10B981",
        "foregroundImage": "./assets/images/Logos/Primary_Emerald.png",
        "backgroundImage": "./assets/images/Logos/Primary_Emerald.png",
        "monochromeImage": "./assets/images/Logos/Monochrome_Dark.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Logos/Primary_Emerald.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logos/Primary_Emerald.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#10B981",
          "dark": {
            "backgroundColor": "#0F172A",
            "image": "./assets/images/Logos/Monochrome_Dark.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
